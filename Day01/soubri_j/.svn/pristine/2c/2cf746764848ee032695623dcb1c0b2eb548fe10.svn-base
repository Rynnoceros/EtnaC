** File description:                                                                                             
**      Function that prints alphabet in lowercase in reverse order                                          
*/
void my_putchar(char c);

void my_rev_alpha(void)
{
    for (unsigned int i = 122; i >= 97; --i) {
        my_putchar(i);
    }
    my_putchar('\n');
}